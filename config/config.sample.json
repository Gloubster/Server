        "address": "local.gloubster",
        "port": 9990
    },
    "listeners": [
        {
            "type": "Gloubster\\Server\\Listener\\ZMQListener",
            "options": {
                "transport": "tcp",
                "address": "127.0.0.1",
                "port": 22345
            }
        },
        {
            "type": "Gloubster\\Server\\Listener\\HTTPListener",
            "options": {
                "host": "0.0.0.0",
                "port": 8990
            }
        }
    ]
